<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:Imview.Core.ViewModels"
             xmlns:templates="clr-namespace:Imview.Core.Controls.Templates"
             mc:Ignorable="d" d:DesignWidth="1000" d:DesignHeight="800"
             x:Class="Imview.Core.Views.QuestTemplateEditorView"
             x:DataType="vm:QuestTemplateEditorViewModel">

    <Grid Background="#1E1E1E" Margin="20">
        <templates:QuestTemplateEditor x:Name="QuestEditor" Template="{Binding Template}"/>
    </Grid>
</UserControl>